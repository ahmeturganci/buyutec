    <script>



        function RolAdiCek(projeId) {
            var veri = {
                projeId: projeId
            };
            $.post("/Proje/Rollerim", veri, function (sonuc) {
               // console.log(sonuc);
                $.each(sonuc, function (j, jtem) {
                    $("#rollu" + projeId).append(jtem);
                });
            });
        }

        function ProjeCek() {


            $.post("/Proje/ProjeCek", function (data) {
                //console.log(data);
                //alert(veri.butce);
                //if(veri.aktifMi==true)
                //    $("#pDetay")
                //if(veri.aktifMi==false)
                //    $("#pDetay").append('<tr> <td>' + veri.projeAdi + '</td> <td>' + veri.olusturmaTarihi + '</td> <td>' + veri.baslangicTarihi + '</td> <td>' + veri.bitisTarihi + '</td><td>' + veri.butce + '</td> <td>ROL</td><td>' + veri.projeAciklama + '</td> <td><span class="label label-danger">Aktif</span></td></tr>');
                $.each(data, function (i, item) {
                    RolAdiCek(item.projeId);
                    if (item.aktifMi == true)
                        $("#pDetay").append('<tr> <td>' + item.projeAdi + '</td> <td>' + item.olusturmaTarihi + '</td> <td>' + item.baslangicTarihi + '</td> <td>' + item.bitisTarihi + '</td><td>' + item.butce + '</td> <td id="rollu' + item.projeId + '"></td><td>' + item.projeAciklama + '</td> <td><span class="label label-success">Aktif</span></td></tr>');
                    if (item.aktifMi == false)
                        $("#pDetay").append('<tr> <td>' + item.projeAdi + '</td> <td>' + item.olusturmaTarihi + '</td> <td>' + item.baslangicTarihi + '</td> <td>' + item.bitisTarihi + '</td><td>' + item.butce + '</td> <td id="rollu"></td><td>' + item.projeAciklama + '</td> <td><span class="label label-danger">Pasif</span></td></tr>');

                });
            });


        }
        function DurumCek() {

            $.post("/Proje/DurumCek", function (data) {
                //console.log(data);
                $("#durumlar").html("");
                $("#ydurumlar").html("");
                $("#adurumlar").html("");
                $.each(data, function (i, item) {
                    //console.log(data);
                    $("#durumlar").append('<option>' + item.durumId + ' ' + item.durumAdi + '</option>');
